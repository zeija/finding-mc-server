    "clean": "node -e \"require('fs').rmSync(require('os').homedir() + '/.minecraft-scanner', {recursive: true, force: true})\"",
    "backup": "node scripts/backup-data.js",
    "export": "node scripts/export-results.js"
  },
  "keywords": [
