  "main": "scanmc.js",
  "bin": {
    "minecraft-scanner": "./scanmc.js"
  },
  "scripts": {
    "start": "node scanmc.js",
    "dev": "node --inspect scanmc.js",
    "test": "node test/scanner.test.js",
